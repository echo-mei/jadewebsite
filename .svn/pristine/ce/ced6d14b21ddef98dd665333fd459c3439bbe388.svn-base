<div class="index-main">
    <div class="tabList-item apply-try-form resume-form" id="applyTryForm">
        <h3 class="item-title">职位申请表</h3>
        <div class="form-item form-item-img">
            <label> </label>
            <div class="file-img">
                <img id="preview" src="../images/add_img.png" alt="照片">
                <input class="file" type="file" id="fileImg" name="image" accept="image/jpg,image/jpeg,image/png,image/bmp,image/gif">
            </div>
        </div>
        <div class="form-item">
            <label><span class="required">*</span>应聘岗位: </label>
            <input type="text" id="applyJob" name="applyJob">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>到岗日期: </label>
            <input class="date" type="text" id="arrivalDate" name="arrivalDate" placeholder="请选择到岗日期" readonly onfocus="$(this).blur()">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>姓名: </label>
            <input type="text" id="name" name="name" placeholder="请输入姓名">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>性别 </label>
            <label class="radio-wrap"><input type="radio" class="require" name="sex" value="1" checked>男</label>
            <label class="radio-wrap"><input type="radio" class="require" name="sex" value="0"/>女</label>
        </div>
        <div class="form-item">
            <label><span class="required">*</span>籍贯: </label>
            <input type="text" id="nativePlace" name="nativePlace" placeholder="请输入籍贯">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>现居住地: </label>
            <input type="text" id="currentAddress" name="currentAddress" placeholder="请输入现居住地">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>邮箱: </label>
            <input type="text" id="email" name="email" placeholder="请输入邮箱">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>手机: </label>
            <input type="text" id="phone" name="phone" placeholder="请输入手机号码">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>QQ/微信: </label>
            <input type="text" id="qq" name="qq" placeholder="请输入QQ号/微信号">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>毕业院校: </label>
            <input type="text" id="graduateSchool" name="graduateSchool" placeholder="请输入毕业院校">
        </div>
        <div class="form-item">
            <label><span class="required">*</span>学历: </label>
            <select id="universityDegree" name="universityDegree">
                <option value="本科">本科</option>
                <option value="硕士">硕士</option>
                <option value="博士">博士</option>
                <option value="MBA">MBA</option>
                <option value="大专">大专</option>
            </select>
        </div>
        <div class="form-item">
            <label><span class="required">*</span>专业: </label>
            <input type="text" id="major" name="major" placeholder="请输入专业">
        </div>
        <!--<div class="form-item">-->
            <!--<label><span class="required">*</span>毕业日期: </label>-->
            <!--<input class="date" type="text" id="graduationDate" name="graduationDate" placeholder="请选择毕业日期" readonly onfocus="$(this).blur()">-->
        <!--</div>-->
        <div class="form-item">
            <label><span class="required">*</span>英语水平: </label>
            <!--<input type="text" id="englishLevel" name="englishLevel" placeholder="请输入英语水平">-->
            <select id="englishLevel" name="englishLevel">
                <!--<option value="无">无</option>-->
                <option value="四级">四级</option>
                <option value="六级">六级</option>
                <option value="其他">其他</option>
            </select>
        </div>
        <!--<div class="form-item">-->
            <!--<label><span class="required">*</span>计算机水平: </label>-->
            <!--<input type="text" id="computerLevel" name="computerLevel" placeholder="请输入计算机水平">-->
        <!--</div>-->
        <div class="form-item">
            <label><span class="required">*</span>所获证书: </label>
            <textarea id="certificate" name="certificate"></textarea>
        </div>
        <div class="form-item">
            <label><span class="required">*</span>在校经历: </label>
            <textarea id="associationActivity" name="associationActivity"></textarea>
        </div>
        <div class="form-item">
            <label><span class="required">*</span>工作经历: </label>
            <textarea id="practiceExperience" name="practiceExperience"></textarea>
        </div>
        <div class="form-item">
            <label><span class="required">*</span>实习经历: </label>
            <textarea id="shixiExperience" name="shixiExperience"></textarea>
        </div>
        <div class="form-item">
            <label>兴趣爱好: </label>
            <textarea id="hobbies" name="hobbies"></textarea>
        </div>
        <div class="form-item">
            <label>自我评价: </label>
            <textarea id="selfEvaluation" name="selfEvaluation"></textarea>
        </div>
        <button type="button" class="form-submit-btn" id="resume_form_btn">提交申请</button>
    </div>
</div>

